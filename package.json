{
  "name": "@sysdoc/generator-sharepoint-app",
  "version": "1.0.0",
  "description": "A yeoman generator for SharePoint projects",
  "homepage": "https://github.com/suhdev/sharepoint-yo-generator#readme",
  "author": "Suhail Abood <suhail.abood@gmail.com>",
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "SharePoint",
    "Generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-sass": "^3.3.7",
    "chalk": "^2.3.0",
    "jira-connector": "^2.8.0",
    "lodash": "^4.17.4",
    "nunjucks": "^3.1.3",
    "request": "^2.87.0",
    "sharepoint-util": "^1.0.56",
    "tough-cookie-file-store": "^1.2.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "url": "git+https://github.com/suhdev/sharepoint-yo-generator.git",
    "type": "git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suhdev/sharepoint-yo-generator/issues"
  }
}
